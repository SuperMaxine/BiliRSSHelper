{
  "name": "BiliRSSHelper",
  "version": "1.0",
  "icons": {
    "128": "128.png",
    "512": "popup.png"
  },
  "description": "Help RSShub sync bilibili's SESSDATA cookie",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "alarms",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "popup.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}